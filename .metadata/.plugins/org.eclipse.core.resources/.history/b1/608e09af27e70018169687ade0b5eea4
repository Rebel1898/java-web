

package com.sls.models;

public class Usuario {

	public  int i;

	public int id;
	private String name;
	private String email;
	private Room room;
	/**
	 * @param id
	 * @param name
	 * @param email
	 */
	public Usuario(int id, String name, String email,Room room) {
		super();
		this.id = id;
		this.name = name;
		this.email = email;
		this.room  = room;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public Room getRoom() {
		return room;
	}
	public void setRoom(Room room) {
		this.room = room;
	}
	
	
	
	
	//Lanzamos la informacion con el request

	
	
	
}
