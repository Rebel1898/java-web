

package com.sls.models;

public class Usuario {

	
	private int id;
	private String name;
	private String email;
	private int num_room;
	/**
	 * @param id
	 * @param name
	 * @param email
	 */
	public Usuario(int id, String name, String email,int num_room) {
		super();
		this.id = id;
		this.name = name;
		this.email = email;
		this.num_room =num_room;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public int getNum_room() {
		return num_room;
	}
	public void setNum_room(int num_room) {
		this.num_room = num_room;
	}
	
	
	
	
	//Lanzamos la informacion con el request

	
	
	
}
